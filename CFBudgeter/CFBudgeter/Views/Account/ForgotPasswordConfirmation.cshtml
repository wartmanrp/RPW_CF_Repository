@{
    ViewBag.Title = "Forgot Password Confirmation";
}

<div id="content-header">
    <h1>@ViewBag.Title.</h1>
</div>
<div id="content-container">
    <p>
        Please check the email you provided and click the provided link to reset your password.
    </p>
    @Html.ActionLink("Back to Login", "Login", "Account", null, new { @class = "btn btn-success "})
</div>
	
